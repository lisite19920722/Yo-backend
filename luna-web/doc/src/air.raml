              }
      404:
          body:
            application/json:
              schema: |
                {
                  "code": 404,
                  "message": "找不到对象"
                }
      500:
        body:
          application/json:
            schema: |
              {
                "code": 500,
                "message": "对不起，你没有该权限，请与系统管理员联系"
              }
  put:
    description: |
      更新一个air_quality模块
    body:
