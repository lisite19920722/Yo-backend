          }
    responses:
      200:
        body:
          application/json:
            example: |
              {
                  "id": "1",
                  "create_time": "2016-2-27 08:00:00",
                  "modify_time": "2016-2-27 10:00:00",
                  "airQualityLevel": 4,
                  "aqi": 106,
                  "co": 1,
                  "date": "2016-1-28 10:00:00",
                  "day": 28,
                  "firstElement": "PM2.5",
                  "hour": 10,
                  "month": 1,
                  "no2": 56,
                  "o3": 21,
                  "pm10": 144,
                  "pm25": 112,
                  "so2": 100,
                  "year": 2016,
                  "air_monitor": 4001
              }
  delete:
    description: |
      删除一个air_quality模块
    responses:
        200:
          body:
            application/json:
              example: |
                {
                  "errorCode": "NO_ERROR",
                  "data": null,
                  "numPerPage": 0,
                  "curPageNum": 0,
                  "totalItemNum": 0,
                  "totalPageNum": 0
                }
        404:
          body:
            application/json:
              schema: |
                {
                  "code": 404,
